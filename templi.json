{
  "excludes": [
    ".git",
    "images",
    ".env.template",
    "README.md",
    "templi.json"
  ],
  "scripts": {
    "before": [],
    "after": [
      "cd {{TEMPLI_OUTPUT_FOLDER}}; rm -rf .git"
    ]
  },
  "placeholders": [
    {
      "label": "Application name:",
      "name": "app_name",
      "type": "TEXT"
    },
    {
      "label": "Author name:",
      "name": "author",
      "type": "TEXT"
    },
    {
      "label": "Project description:",
      "name": "description",
      "type": "TEXT"
    },
    {
      "label": "Select license:",
      "name": "license",
      "type": "SELECT",
      "choices": [
        "MIT",
        "ISC"
      ]
    },
    {
      "label": "Is it private?",
      "name": "private",
      "type": "BOOLEAN"
    },
    {
      "label": "Version number:",
      "name": "version",
      "type": "TEXT"
    }
  ]
}
