{
  "excludes": [
    ".env.template",
    "README.md",
    "templi.json"
  ],
  "scripts":{
    "before":[],
    "after": [
      "cd {{TEMPLI_OUTPUT_FOLDER}}; npm install"
    ]
  },
  "keys": [
    {
      "label": "Application name:",
      "name": "app_name",
      "type": "INPUT"
    },
    {
      "label": "Author name:",
      "name": "author",
      "type": "INPUT"
    },
    {
      "label": "Project description:",
      "name": "description",
      "type": "INPUT",
      "clean": false
    },
    {
      "label": "Select license:",
      "name": "license",
      "type": "SELECT",
      "choices": [
        "MIT",
        "ISC"
      ]
    },
    {
      "label": "Is it private?",
      "name": "private",
      "type": "BOOLEAN",
      "default": false
    },
    {
      "label": "Version number:",
      "name": "version",
      "type": "INPUT"
    }
  ]
}